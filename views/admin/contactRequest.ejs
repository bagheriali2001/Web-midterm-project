<%- include('../includes/head.ejs') %>

    </head>

    <body>
        <%- include('../includes/adminNavigation.ejs') %>
        <main class="float-left w-100">
            <section class="news float-left w-100 py-4">
                <div class="container">
                    <div class="news-content float-left w-100">
                        <% for(var i = 0; i < contacts.length; i++) { %>
                        <div class="news-card col-12 col-md-6 col-lg-3 card float-left px-2 border-0 py-3 bg-light" style="height: 450px;">
                            <div class="card-body">
                                <p class="card-text">نام و نام خانوادگی: </p>
                                <p class="card-text"><%= contacts[i].name %></p>
                                <p class="card-text">موضوع:</p>
                                <p class="card-text"><%= contacts[i].subject %></p>
                                <p class="card-text">ایمیل:</p>
                                <p class="card-text"><%= contacts[i].email %></p>
                                <p class="card-text">متن نظر:</p>
                                <p class="card-text"><%= contacts[i].text %></p>
                            </div>
                        </div>
                        <% } %>
                    </div>
                </div>  
            </section>
                       
        </main>

        <%- include('../includes/footer.ejs') %>

<%- include('../includes/end.ejs') %>
            